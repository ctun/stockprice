<h2> Enter Symbol and Price </h1>


<% if @history.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@history.errors.count, "error") %> prohibited
        this article from being saved:
      </h2>
      <ul>
        <% @history.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>


<%= form_for :history, url: histories_path do |f| %>
  <p>
    <%= f.label :symbol %><br>
    <%= f.text_field :symbol %>
  </p>
 
  <p>
    <%= f.label :price %><br>
    <%= f.number_field :price, :step => "0.01" %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

